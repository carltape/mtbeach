Cell["\<\
The preceding is an XISO ref matrix if the third and fourth diagonal entries \
are the same . 
(The other entries are already what we want .)\
\>", "Subsubsection",
 CellChangeTimes->{{3.911248486172781*^9, 
  3.911248569055188*^9}},ExpressionUUID->"26f6c1a5-c100-4cc0-8e15-\
19888eb73c9d"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"a", "'"}], "-", 
     RowBox[{
      RowBox[{"a", "'"}], " ", "t"}], "+", 
     RowBox[{"c", " ", "t"}]}], "==", 
    RowBox[{
     RowBox[{"d", "'"}], "+", 
     RowBox[{"d", " ", "t"}], "-", 
     RowBox[{
      RowBox[{"d", "'"}], " ", "t"}]}]}], ",", "t"}], "]"}]], "Input",
 CellChangeTimes->{{3.911247138268775*^9, 3.911247180888568*^9}, {
  3.911247373261389*^9, 3.9112473862375107`*^9}},
 CellLabel->
  "In[3064]:=",ExpressionUUID->"821eb383-3066-47c4-899f-c67b82503490"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{"t", "\[Rule]", 
    FractionBox[
     RowBox[{
      SuperscriptBox["a", "\[Prime]",
       MultilineFunction->None], "-", 
      SuperscriptBox["d", "\[Prime]",
       MultilineFunction->None]}], 
     RowBox[{
      RowBox[{"-", "c"}], "+", "d", "+", 
      SuperscriptBox["a", "\[Prime]",
       MultilineFunction->None], "-", 
      SuperscriptBox["d", "\[Prime]",
       MultilineFunction->None]}]]}], "}"}], "}"}]], "Output",
