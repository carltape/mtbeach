Cell[TextData[{
 "Find the TET ref matrix for your TET endpoint.  \nIt will be  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"[", 
      OverscriptBox["U", "_"], "]"}], "T"], "\[Times]"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c294cad0-52ee-4f3b-b36b-c076866fc729"],
 "(your TET matrix)\[Times] ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    OverscriptBox["U", "_"], "]"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "cb06e00e-9ecd-4f6d-889c-4977792be2af"],
 ", where U is your U for nm1."
}], "Subsubsection",
 CellChangeTimes->{{3.911248486172781*^9, 3.911248569055188*^9}, {
  3.911248688736018*^9, 3.9112487514698544`*^9}, {3.911248796837945*^9, 
  3.9112489987729306`*^9}, {3.91124976504329*^9, 
  3.911249799981164*^9}},ExpressionUUID->"82d9dcee-3029-4708-b432-\
026ef01bb432"],

Cell[TextData[{
 "Find the CUBE ref matrix for your CUBE endpoint.  \nIt will be  ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"[", 
     OverscriptBox["U", "_"], "]"}], "T"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8381b96c-4557-4366-91e0-2ed79cf08f46"],
 "\[Times](your CUBE matrix) \[Times]",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    OverscriptBox["U", "_"], "]"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "4a3288d1-8a45-42fc-b4ab-94058a680b06"],
 ", where U is your U for nm1."
}], "Subsubsection",
 CellChangeTimes->{{3.911248486172781*^9, 3.911248569055188*^9}, {
  3.911248688736018*^9, 3.9112487514698544`*^9}, {3.911248796837945*^9, 
  3.9112489987729306`*^9}, {3.9112490312868834`*^9, 3.9112490475102234`*^9}, {
  3.9112497882581625`*^9, 
  3.91124982493708*^9}},ExpressionUUID->"58812f7b-8eb2-4f60-a55f-\
dd847c3364de"],

Cell[TextData[{
 "Solve for t as above. That should give you the XISO ref matrix for the \
matrix you want.\nThen conjugate by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    OverscriptBox["U", "_"], "]"}], TraditionalForm]],ExpressionUUID->
  "983924e9-7ed5-4685-9044-41a8dab24dd6"],
 " to get the actual matrix you want, not just the ref matrix."
}], "Subsubsection",
