Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"pg", "=", "3"}], ";", 
   RowBox[{"ag", "=", "3"}], ";"}], " ", 
  RowBox[{"(*", " ", 
   StyleBox[
    RowBox[{
    "controls", " ", "the", " ", "accuracy", " ", "of", " ", "the", " ", 
     "numerical", " ", "calculations"}],
    FontColor->RGBColor[1, 0, 0]], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"MeanSphere", "[", 
    RowBox[{"f_", ",", "Tmat_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"4", " ", "\[Pi]"}]], 
    RowBox[{"NIntegrate", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"f", "[", 
        RowBox[{"\[Theta]", ",", "\[Phi]", ",", "Tmat"}], "]"}], 
       RowBox[{"Sin", "[", "\[Phi]", "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"\[Theta]", ",", "0", ",", 
        RowBox[{"2", "\[Pi]"}]}], "}"}], ",", 
