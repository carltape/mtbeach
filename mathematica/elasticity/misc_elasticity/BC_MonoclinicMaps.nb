    RowBox[{
     RowBox[{"temp", "[", 
      RowBox[{"Tmat", ",", "\[CapitalSigma]"}], "]"}], "=", " ", 
     RowBox[{"NMaximize", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"DistToV\[CapitalSigma]ofU", "[", 
