           RowBox[{"UsHat", "[", 
            RowBox[{"{", 
             RowBox[{"\[Theta]", ",", "\[Sigma]", ",", "\[Phi]"}], "}"}], 
            "]"}], ",", "\[CapitalSigma]"}], "]"}], ",", 
         RowBox[{
          RowBox[{"0", "\[LessEqual]", "\[Theta]", "\[LessEqual]", 
           RowBox[{"2", "\[Pi]"}]}], "&&", 
          RowBox[{
           RowBox[{"-", "\[Pi]"}], "\[LessEqual]", "\[Sigma]", "\[LessEqual]",
            "\[Pi]"}], "&&", 
