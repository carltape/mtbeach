Cell[BoxData[
 RowBox[{
  RowBox[{"gettwopts", "[", "pts_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "segment", ",", "tslope", ",", "yIntercept", ",", "tStart", ",", "tEnd", 
      ",", "yStart", ",", "yEnd", ",", "lpts"}], "}"}], ",", 
    RowBox[{
     RowBox[{"segment", "=", 
      RowBox[{
       RowBox[{"Select", "[", 
        RowBox[{
         RowBox[{"Partition", "[", 
          RowBox[{"pts", ",", "2", ",", "1"}], "]"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"#", "[", 
            RowBox[{"[", 
             RowBox[{"2", ",", "1"}], "]"}], "]"}], "==", "1"}], "&"}]}], 
        "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"tslope", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"segment", "[", 
          RowBox[{"[", 
           RowBox[{"2", ",", "2"}], "]"}], "]"}], "-", 
         RowBox[{"segment", "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", "2"}], "]"}], "]"}]}], ")"}], "/", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"segment", "[", 
          RowBox[{"[", 
           RowBox[{"2", ",", "1"}], "]"}], "]"}], "-", 
         RowBox[{"segment", "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"yIntercept", "=", 
      RowBox[{
       RowBox[{"segment", "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", "2"}], "]"}], "]"}], "-", 
       RowBox[{"tslope", " ", 
        RowBox[{"segment", "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tStart", "=", 
      RowBox[{
       RowBox[{"First", "[", "pts", "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"tEnd", "=", 
      RowBox[{
       RowBox[{"Last", "[", "pts", "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"yStart", "=", 
      RowBox[{
       RowBox[{"tslope", " ", "tStart"}], "+", "yIntercept"}]}], ";", 
