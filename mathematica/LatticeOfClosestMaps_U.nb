  3.90295774783969*^9}},ExpressionUUID->"84ea08fc-304e-429c-baa3-\
8dc4ba681a1a"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"U", "=", 
   RowBox[{"UT", "[", 
    RowBox[{
     RowBox[{"Closest", "[", 
      RowBox[{"Tmat", ",", "XISO"}], "]"}], ",", "XISO"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
