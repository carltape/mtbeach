  FormBox[
   RowBox[{
    SubscriptBox["\[Beta]", "ISO"], "=", 
    SuperscriptBox["26.39", 
     StyleBox["o",
      FontSlant->"Plain"]]}], TraditionalForm]],ExpressionUUID->
  "ac46f81e-1fc9-4289-b508-ed50fcb1b437"],
 ", VTI: ",
