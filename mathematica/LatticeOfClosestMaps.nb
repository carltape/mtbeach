Cell[BoxData[
 RowBox[{"(*", " ", 
  RowBox[{
   RowBox[{"GetTempAnd\[Theta]0\[Sigma]0\[Phi]0", "[", 
    RowBox[{
     StyleBox["Tmat_",
      FontColor->GrayLevel[0]], 
     StyleBox[",",
      FontColor->GrayLevel[0]], 
     StyleBox["MONO",
      FontColor->RGBColor[1, 0, 0]]}], "]"}], ":=", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"Clear", "[", 
      RowBox[{"\[Theta]", ",", "\[Sigma]", ",", "\[Phi]"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
