Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"AA", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\[Theta]_", ",", "\[Sigma]_"}], "}"}], ",", "\[Gamma]_"}], 
    "]"}], ":=", 
   RowBox[{
    FractionBox["1", "8"], 
    RowBox[{"(", 
     RowBox[{"6", "+", 
      RowBox[{"2", " ", 
       RowBox[{"Cos", "[", 
        RowBox[{"2", " ", "\[Theta]"}], "]"}], " ", 
       RowBox[{"Cos", "[", 
        RowBox[{"2", " ", "\[Sigma]"}], "]"}]}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"3", "+", 
         RowBox[{"3", 
          RowBox[{"Cos", "[", 
           RowBox[{"2", " ", "\[Theta]"}], "]"}]}], "+", 
         RowBox[{"3", 
          RowBox[{"Cos", "[", 
           RowBox[{"2", " ", "\[Sigma]"}], "]"}]}], "-", 
         RowBox[{
          RowBox[{"Cos", "[", 
           RowBox[{"2", " ", "\[Theta]"}], "]"}], 
          RowBox[{"Cos", "[", 
           RowBox[{"2", " ", "\[Sigma]"}], "]"}]}]}], ")"}], 
       RowBox[{"Cos", "[", 
        RowBox[{"2", "\[Gamma]"}], "]"}]}], "+", 
      RowBox[{
       SqrtBox["3"], " ", 
       RowBox[{"(", 
        RowBox[{"3", "-", 
         RowBox[{"Cos", "[", 
          RowBox[{"2", " ", "\[Theta]"}], "]"}], "-", 
         RowBox[{"Cos", "[", 
          RowBox[{"2", " ", "\[Sigma]"}], "]"}], "-", 
         RowBox[{
          RowBox[{"Cos", "[", 
           RowBox[{"2", " ", "\[Theta]"}], "]"}], " ", 
          RowBox[{"Cos", "[", 
           RowBox[{"2", " ", "\[Sigma]"}], "]"}]}]}], ")"}], 
       RowBox[{"Sin", "[", 
        RowBox[{"2", "\[Gamma]"}], "]"}]}]}], ")"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"BB", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\[Theta]_", ",", "\[Sigma]_"}], "}"}], ",", "\[Gamma]_"}], 
    "]"}], ":=", 
   RowBox[{
    RowBox[{"-", 
     SuperscriptBox[
      RowBox[{"Sin", "[", 
       RowBox[{"\[Gamma]", "-", 
        RowBox[{"\[Pi]", "/", "6"}]}], "]"}], "2"]}], " ", 
    RowBox[{"Sin", "[", 
